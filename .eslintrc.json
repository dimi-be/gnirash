{
    "parser": "typescript-eslint-parser",
    "env": {
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "rules": {
        "semi": ["error", "never"],
        "no-underscore-dangle": ["error", { "allowAfterThis": true }]
    },
    "settings": {
        "import/parsers": {
            "typescript-eslint-parser": [".ts", ".tsx"]
        }
    }
}
