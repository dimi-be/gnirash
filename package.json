{
  "name": "gnirahs",
  "version": "1.0.0",
  "description": "Easily share directories over the internet with a simple web interface. No database requiered. Features users, downloading and uploading.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "ncu": "./node_modules/npm-check-updates/bin/ncu",
    "ncu-u": "./node_modules/npm-check-updates/bin/ncu -u",
    "build": "mkdir -p bin && npm run build-tsc && npm run build-copyfiles",
    "build-tsc": "./node_modules/.bin/tsc --project ./tsconfig.json",
    "build-copyfiles": "find . \\( -name *.css -o -name *.pug  -o -name *.gif \\) -a -not -path './node_modules/*' -a -not -path './bin/*' -exec cp -f --parents {} ./bin \\;"
  },
  "author": "Dimitri Michaux",
  "license": "ISC",
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.47",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-alias": "^1.0.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^4.0.1",
    "mock-require": "^2.0.2",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "command-line-args": "^4.0.7",
    "cookie-parser": "^1.4.3",
    "date-format": "^1.1.0",
    "date-time": "^2.1.0",
    "error-stack-parser": "^2.0.1",
    "express": "^4.16.2",
    "filesize": "^3.5.11",
    "jsonwebtoken": "^8.1.0",
    "mime-types": "^2.1.17",
    "npm-check-updates": "^2.13.0",
    "pug": "^2.0.0-rc.4",
    "winston": "^2.4.0"
  }
}
